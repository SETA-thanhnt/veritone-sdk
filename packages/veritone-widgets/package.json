{
  "name": "veritone-widgets",
  "version": "0.0.1",
  "dependencies": {
    "es6-promise": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-redux-saga": "^1.0.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "veritone-react-common": "^1.1.0",
    "veritone-redux-common": "^2.0.0"
  },
  "main": "dist/bundle-umd.js",
  "module": "dist/bundle-es.js",
  "jsnext:main": "dist/bundle-es.js",
  "scripts": {
    "start": "yarn run start:widget-server; yarn run start:storybook",
    "start:widget-server": "yarn workspace veritone-widgets-server run start &",
    "start:storybook": "start-storybook -p 3000 -c .storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"./src/**/*.{js,scss}\"",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress",
    "lint": "eslint src/** --ext .js"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "3.3.0-alpha.4",
    "@storybook/react": "v3.3.0-alpha.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-lodash": "^2.5.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "redux-validate-fsa": "^0.1.2",
    "rollup": "^0.52.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-import-alias": "^1.0.4",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-resolve-aliases": "^0.2.0",
    "rollup-plugin-url": "^1.3.0",
    "veritone-dev-env": "^1.0.2",
    "webpack": "^3.6.0"
  },
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/testSuitePolyfills.js",
      "<rootDir>/test/configureEnzyme.js"
    ]
  }
}
